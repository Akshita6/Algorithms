class Solution {
    public boolean isPerfectSquare(int num) {
        long temp = 0;
        for(long i = 0; ;i++)
        {
            temp = i * i;
            if(temp == num)
            {
                return true;
            }
            if(temp > num){
                break;
            }
        }
        return false;

    }
}

